   "source": [
    "# Sample text to tokenize\n",
    "text = \"Hello ! My name is Rahul Yadav I live in mumbai\"\n",
    "\n",
    "# Tokenizing the text using split()\n",
    "tokens = text.split()\n",
    "\n",
    "# Printing the tokens\n",
    "print(\"=\"*60)\n",
    "print(\"Tokenization using Python’s split() function\")\n",
    "print(\"-\"*10)\n",
    "print(\"Tokens:\", tokens)\n",
    "print(\"=\"*60)\n"
   ]
